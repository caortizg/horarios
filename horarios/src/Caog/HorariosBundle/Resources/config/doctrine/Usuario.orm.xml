<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Caog\HorariosBundle\Entity\Usuario" table="usuario">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apellidos" type="string" column="apellidos" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="edad" type="integer" column="edad" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="pais" type="string" column="pais" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="departamento" type="string" column="departamento" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ciudad" type="string" column="ciudad" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="localidad" type="string" column="localidad" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="direccion" type="string" column="direccion" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
